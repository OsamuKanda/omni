    #divLogin
    {
        /*display: none;*/
        width: 100%;
        height: 100%;
    }
    #divLoginTBL
    {
        width: 250px;
        height: 150px;
        margin: auto;
        overflow: hidden;
    }
    .Messege
    {
        text-align: left;
        color: #FF0000;
    }
    table
    {
       border: 2px solid #C0C0C0;
       border-collapse: collapse;
       background-color: #FFFFFF;
    }
    th
    {
        height: 25px;
        border: solid 1px #EEEEEE;
        color: #FFFFFF;
        background-color: #5D7B9D;
    }
    td
    {
        height: 25px;
        border: solid 1px #EEEEEE;
        padding-left: 4px;
        padding-right: 4px;
    }
    .UserID
    {
        border-style: inset;
        width: 140px;
    }
    .Password
    {
        border-style: inset;
        width: 140px;
    }
